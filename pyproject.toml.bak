[project]
name = "poetry-demo"
version = "0.1.0"
description = ""
authors = [
    {name = "hyunbae.jeon",email = "mazingazz@gmail.com"}
]
readme = "README.md"
requires-python = ">=3.10,<3.15"
dependencies = [
    "poetry-core (>=2.1.3)",
    "fastapi (>=0.116.1,<0.117.0)",
    "uvicorn[standard] (>=0.35.0,<0.36.0)",
    "oracledb (>=3.2.0,<4.0.0)",
    "isort (>=6.0.1,<7.0.0)",
    "sqlalchemy (>=2.0.42,<3.0.0)",
    "sqlalchemy-utils (==0.41.1)",
    "pydantic (>=2.11.7,<3.0.0)",
    "pandas (>=2.3.1,<3.0.0)",
    "asyncpg (>=0.30.0,<0.31.0)",
    "orjson (>=3.11.1,<4.0.0)",
    "langchain[openai] (>=0.3.27,<0.4.0)",
    "langchain-openai (>=0.3.30,<0.4.0)",
    "langchain-community (>=0.3.27,<0.4.0)",
    "langchain-core (>=0.3.74,<0.4.0)",
    "langsmith (>=0.3.45,<0.4.0)",
    "python-dotenv (>=1.1.1,<2.0.0)",
    "openai (>=1.101.0,<2.0.0)",
    "bs4 (>=0.0.2,<0.0.3)",
    "faiss-cpu (>=1.12.0,<2.0.0)",
    "sentence-transformers (>=5.1.0,<6.0.0)",
    "mcp (>=1.3.0,<2.0.0)",
    "python-jose[cryptography] (>=3.3.0,<4.0.0)",
    "passlib[bcrypt] (>=1.7.4,<2.0.0)",
    "loguru (>=0.7.0,<0.8.0)",
    "llama-index (>=0.12.0,<0.13.0)",
    "pymupdf (>=1.24.0,<2.0.0)",
    "redis (>=5.0.0,<6.0.0)",
]

[tool.poetry]
packages = [{include = "poetry_demo", from = "src"}]


[tool.poetry.group.dev.dependencies]
pre-commit = "^4.2.0"
black = "^25.1.0"
pydantic-settings = "^2.10.1"
pytest = "^8.0.0"
pytest-asyncio = "^0.23.0"
httpx = "^0.27.0"
pytest-mock = "^3.14.0"

[build-system]
requires = ["poetry-core>=1.0.0", "setuptools>=77.0.3"]
build-backend = "poetry.core.masonry.api"

[tools.setuptools]
packages = ["src"]

[tool.poetry.scripts]
start = 'src.main:start'
mcp-server = 'src.mcp_server:main'
mcp-example = 'src.examples.mcp_client_example:main'
async-patterns = 'src.examples.async_patterns:main'
lcel-patterns = 'src.examples.lcel_patterns:main'
llamaindex-patterns = 'src.examples.llamaindex_patterns:main'